{
  "name": "ng-chuck-norris",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "yarn run gen:jsonplapi && ng build",
    "build:prod": "yarn run gen:jsonplapi && ng build --configuration production --aot --base-href / ",
    "test": "ng test",
    "e2e": "ng e2e",
    "semantic-release": "semantic-release",
    "lint": "yarn run lint:code && yarn run lint:style",
    "lint:code": "ng lint",
    "lint:style": "stylelint src/**/*.scss",
    "fix": "yarn run fix:code && yarn run fix:style",
    "fix:code": "prettier --write \"src/**/*.{ts,js,html}\"",
    "fix:code:staged": "pretty-quick --staged",
    "fix:prettier:staged": "pretty-quick --staged",
    "fix:style": "stylelint src/**/*.scss --fix",
    "gen:jsonplapi": "node_modules/.bin/rimraf generated-sources/api/jsonpl/ && npx @openapitools/openapi-generator-cli generate -i api-spec/json-placeholder.yaml -g typescript-angular -o generated-sources/api/jsonpl/ --additional-properties=ngVersion=12.2.17",
    "cypress:open": "ng run ng-chuck-norris:cypress-open",
    "cypress:run": "ng run ng-chuck-norris:cypress-open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.17",
    "@angular/common": "~12.2.17",
    "@angular/compiler": "~12.2.17",
    "@angular/core": "~12.2.17",
    "@angular/forms": "~12.2.17",
    "@angular/platform-browser": "~12.2.17",
    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular/router": "~12.2.17",
    "@asymmetrik/ngx-leaflet": "^8.1.0",
    "@cds/core": "^5.1.0",
    "@clr/angular": "^5.1.1",
    "@clr/icons": "^5.1.1",
    "@clr/ui": "^5.1.1",
    "eslint-plugin-rxjs": "^3.1.1",
    "leaflet": "^1.7.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.17",
    "@briebug/cypress-schematic": "^5.0.1",
    "@openapitools/openapi-generator-cli": "^2.2.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/leaflet": "^1.7.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "cypress": "latest",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "^3.3.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "semantic-release": "^17.4.2",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaddoz/ng-chuck-norris.git"
  }
}
